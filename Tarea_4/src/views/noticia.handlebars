{{#each article}}
    <hr>
    <table width="40%">
        <tr>
            <td><h1>{{this.title}}</h1></td>
        </tr>
        <tr>
            <td><h4>{{this.description}}</h4></td>
        </tr>
        <tr>
            <td><img src="{{this.urlToImage}}" class="image"></td>
        </tr>
        <tr>
            <td><a href={{this.url}} target="_blank">{{this.url}}</a></td>
        </tr>
    </table>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
{{/each}}